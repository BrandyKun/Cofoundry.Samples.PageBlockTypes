@model DirectoryListDisplayModel

@Cofoundry.BlockType.UseDescription("A list of pages in a specific directory")

@if (Model.Pages.Items.Any())
{
<ul>
    @foreach (var pageItem in Model.Pages.Items)
    {
        <li><a href="@Cofoundry.Routing.Page(pageItem)">@pageItem.Title</a></li>
    }
</ul>
}